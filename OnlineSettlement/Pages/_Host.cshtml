@page "/"
@using Microsoft.AspNetCore.Components.Web
@namespace OnlineSettlement.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <base href="~/" />
    <link href="css/site.css" rel="stylesheet" />
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
     <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">

    <script src="scripts/datepicker.custom.js" suppress-error="BL9992"></script>
    <script src="scripts/vue.js" suppress-error="BL9992"></script>
@*     <script src="scripts/datepicker_select.js" suppress-error="BL9992"></script> *@
    <script src="scripts/datepickerLanguage.js" suppress-error="BL9992"></script>

